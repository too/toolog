// Custom styles.

// zetabee.com embed ical css
.ij_wrap {
    /* font-family:sans-serif; */
    font-size:12px;
    /* width:300px; */
    overflow:auto;
    overflow-x:hidden;
    overflow-y:auto;
}

.ij_wrap a, .ij_wrap a:hover {
    text-decoration:none;
}

.ij_table {
    table-layout: auto;
    margin: 0;
    font-size:12px;
    border-collapse:collapse;
    width:100%;
}

.ij_date {
    background:#fff;
}

.ij_date th {
    text-align:left;
    padding:14px 4px 4px 4px;
    font-size:13px;
    border-bottom: 1px solid #ddd;
}

.ij_row_a, .ij_row_b {
    border-top:1px solid #ccf;
    border-bottom:1px solid #ccf;
}

.ij_row_a {
}

.ij_row_b {
}

.ij_cal01 {
    background:#f4f4ff;
}
.ij_cal01 .ij_text:before {
    content:">> ";
    padding-right:0;
    color:#449;
    font-size:12px;
}

.ij_cal02 {
    background:#f4fff4;
}
.ij_cal02 .ij_text:before {
    content:"";
    padding-right:0;
    color:#494;
    font-size:12px;
}

.ij_cal03 {
    background:#fff4f4;
}
.ij_cal03 .ij_text:before {
    content:"";
    padding-right:0;
    color:#944;
    font-size:12px;
}

.ij_cal04 {
    background:#f4f4f4;
}

.ij_cal05 {
    background:#ffffe0;
}

.ij_time, .ij_desc {
    line-height:16px;
}

.ij_time {
    min-width:50px;
    width:50px;
    text-align:right;
    padding-right:6px;
    color:#666;
}

.ij_time div {
}

.ij_desc a {
    width:125px;
    height:16px;
    overflow:hidden;
    cursor:pointer;
    outline:none;
    color: #555;
  display: block;
  font-size: 12px;
  border: 0px solid #f4f4ff;
  text-decoration: none;
}

.ij_desc span {
    display:block;
}

.ij_text {
}

.ij_table td {
    padding: 8px 4px 8px 4px;
    border-bottom-width: 0px;
}

.ij_text u:hover {
    color:blue;
}

.ij_loc {
    margin-top:6px;
    font-size:11px;
    color:#060;
}

.ij_credit {
    display: none;
    text-align:center;
    padding:12px 0 4px 0;
    background:#fff;
    font-size:9px;
    color:#777;
}

.ij_credit a, .ij_credit a:hover {
    color:#447;
    font-weight:bold;
}

.cal-link {
    margin-top: 20px;
}

.cal-link a {
    border: none;
    color: inherit;
}